@model EFDataApp.Domain.Entities.Store
@{
    ViewBag.Title = "Редактирование склада";
}
<form asp-action="edit" asp-controller="store" asp-route-id="@Model.Id">
    <div class="form-group">
        <label asp-for="Location" class="control-label">Город</label>
        <input type="text" asp-for="Location" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Сохранить" class="btn btn-default" />
    </div>
</form>
